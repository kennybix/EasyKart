@model IEnumerable<Order>
<h1>Manage Orders</h1>
<table>
    <thead>
        <tr>
            <th>User</th>
            <th>Date</th>
            <th>Total Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.User.Username</td>
                <td>@order.OrderDate</td>
                <td>@order.TotalAmount</td>
            </tr>
        }
    </tbody>
</table>
